{% extends 'template/admin/_post.twig' %}
{% block page_name %}Extension{% endblock %}
{% block body %}
  <section class="content">
      <div class="card">
        <div class="card-header">
          <h4>{{ Feature }}</h4>
          <p>{{ Description }}</p>
          <a href="{{ '/admin/extension/enable' | base_url }}?name={{extension_name}}" class="btn btn-secondary">Enable</a>
        </div>
      </div>
  </section>
{% endblock %}